SELECT BCREFER
DEFI WIND BRSER FROM LIBRREF,CIBRREF TO LIBRREF+10,CIBRREF+45;
                TITLE " REFERENCIAS" ;
                FOOTER " ESC=Seleccionar ";
                DOUBLE NOFLOAT NOZOOM NOGROW SHADOW COLOR SCHEME 10
ACTI WIND BRSER
BROWSE FIELDS CODIGO:H="CODIGO ITEM", DESCRI:H="DESCRIPCION";
              NOAPPEND NODELETE NOEDIT NOMENU NOOPTIMIZE REST SAVE IN WINDOW BRSER
              *FOR ALMACEN=WATENCION
RELEASE WIND BRSER
RETURN
